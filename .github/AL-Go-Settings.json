{
  "type": "PTE",
  "templateUrl": "https://github.com/microsoft/AL-Go-PTE@preview",
  "incrementalBuilds": {
    "onPush": true,
    "onPull_Request": true,
    "onSchedule": false,
    "retentionDays": 60,
    "mode": "modifiedApps"
  },
  "deployToBCLE": {
    "projects": "DK",
    "dependencyInstallMode": "upgrade",
    "includeTestAppsInSandboxEnvironment": true,
    "excludeAppIds": [
      "fce99180-46e1-472f-9e49-097cd4298b90",
      "ba0f4b41-ce6b-403a-aff1-ad045b241979"
    ]
  },
  "buildModes": [
    "Default",
    "NextMajor"
  ],
  "preprocessorSymbols": [
    "GLOBAL"
  ],
  "conditionalSettings": [
    {
      "workflows": [
        "CICD"
      ],
      "settings": {
        "workflowConcurrency": [
          "group: ${{ github.workflow }}-${{ github.ref }}",
          "cancel-in-progress: true"
        ]
      }
    },
    {
      "branches": [
        "main"
      ],
      "settings": {
        "preprocessorSymbols": [
          "MAIN"
        ]
      }
    },
    {
      "buildModes": [
        "NextMajor"
      ],
      "settings": {
        "artifact": "////nextmajor",
        "versioningStrategy": 15,
        "preprocessorSymbols": [
          "NEXTMAJOR"
        ]
      }
    }
  ],
  "useProjectDependencies": true,
  "runs-on": "ubuntu-latest",
  "shell": "pwsh",
  "gitHubRunner": "windows-latest",
  "githubRunnerShell": "pwsh",
  "generateDependencyArtifact": true,
  "versioningStrategy": 3,
  "templateSha": "6e5bf7752c8288a922c50393814925d794e9ec38"
}
